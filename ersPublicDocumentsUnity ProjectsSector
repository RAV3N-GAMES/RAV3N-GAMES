[32m+[m[32m    bool isDisplay = false; //내려놓았을 때 성공한 위치인지[m
[32m+[m
[32m+[m[32m    bool isResult = false;[m
[32m+[m
[32m+[m[32m    RoomManager roomManager;[m
[32m+[m[32m    DayandNight dayAndNight;[m
[32m+[m[32m    ResultPopUp resultPopUp;[m
[32m+[m
[32m+[m[32m    public enum TaskType[m
[32m+[m[32m    {[m
[32m+[m[32m        Arrow,[m
[32m+[m[32m        Select,[m
[32m+[m[32m        LongSelect,[m
[32m+[m[32m        Display,[m
[32m+[m[32m        MoveMap,[m
[32m+[m[32m        Fight[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    bool IsDone()[m
[32m+[m[32m    {[m
[32m+[m[32m        return isDone;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void InitTask(RoomManager room, ResultPopUp result, DayandNight dayandNight)[m
[32m+[m[32m    {[m
[32m+[m[32m        if (roomManager == null)[m
[32m+[m[32m        {[m
[32m+[m[32m            roomManager = room;[m
[32m+[m[32m            dayAndNight = dayandNight;[m
[32m+[m[32m            resultPopUp = result;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        DisplayIdx = 0;[m
[32m+[m[32m        ButtonIdx = 0;[m
[32m+[m[32m        TutorialTileIdx = 0;[m
[32m+[m
[32m+[m[32m        Cnt = 0;[m
[32m+[m[32m        StartCoroutine(TakeTask());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    IEnumerator TakeTask()[m
[32m+[m[32m    {[m
[32m+[m[32m        TaskManager.isDone = false;[m
[32m+[m[41m        [m
[32m+[m[32m        while(Cnt < TaskList.Count)[m
[32m+[m[32m        {[m
[32m+[m[32m            print("Cnt" + Cnt);[m
[32m+[m[32m            isDone = false;[m
[32m+[m[32m            switch (TaskList[Cnt])[m
[32m+[m[32m            {[m
[32m+[m[32m                case TaskType.Arrow: StartCoroutine(Arrow()); break;[m
[32m+[m[32m                case TaskType.Display: StartCoroutine(Display()); break;[m
[32m+[m[32m                case TaskType.Fight: StartCoroutine(Fight()); break;[m
[32m+[m[32m                case TaskType.LongSelect: StartCoroutine(LongSelect()); break;[m
[32m+[m[32m                case TaskType.MoveMap: StartCoroutine(MoveMap()); break;[m
[32m+[m[32m                case TaskType.Select: StartCoroutine(Select()); break;[m
[32m+[m[32m            }[m
[32m+[m[32m            Cnt++;[m
[32m+[m[32m            yield return new WaitUntil(IsDone);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        TaskManager.isDone = true;[m
[32m+[m[32m        TaskManager.taskManager.Dialog.SetActive(true);[m
[32m+[m
[32m+[m[32m        yield break;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    IEnumerator Arrow()[m
[32m+[m[32m    {[m
[32m+[m[32m        for (int i = 0; i < 3; i++)[m
[32m+[m[32m        {[m
[32m+[m[32m            DisplayObject[DisplayIdx].SetActive(!DisplayObject[DisplayIdx].activeInHierarchy);[m
[32m+[m[32m            yield return new WaitForSeconds(TaskManager.taskManager.arrowSpeed);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        DisplayIdx++;[m
[32m+[m
[32m+[m[32m        yield return new WaitForSeconds(TaskManager.taskManager.clickTime);[m
[32m+[m[32m        isDone = true;[m
[32m+[m[32m        yield break;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void SetSlotStatus(bool isPossible) //설치를 위한 슬롯 세팅[m
[32m+[m[32m    {[m
[32m+[m[32m        ClickSlot clickSlot = ButtonList[ButtonIdx].GetComponent<ClickSlot>();[m
[32m+[m
[32m+[m[32m        ButtonList[ButtonIdx].GetComponent<Button>().enabled = !isPossible;[m
[32m+[m[32m        clickSlot.isDisplay = isPossible;[m
[32m+[m
[32m+[m[32m        if (isPossible)[m
[32m+[m[32m        {[m
[32m+[m[32m            clickSlot.taskObject = this;[m
[32m+[m
[32m+[m[32m            ButtonParent = ButtonList[ButtonIdx].parent;[m
[32m+[m[32m            ButtonSiblingIdx = ButtonList[ButtonIdx].GetSiblingIndex();[m
[32m+[m
[32m+[m[32m            ButtonList[ButtonIdx].SetParent(TaskManager.taskManager.Mask);[m
[32m+[m[32m        }[m
[32m+[m[32m        else[m
[32m+[m[32m        {[m
[32m+[m[32m            clickSlot.taskObject = null;[m
[32m+[m
[32m+[m[32m            ButtonList[ButtonIdx].SetParent(ButtonParent);[m
[32m+[m[32m            ButtonList[ButtonIdx].SetSiblingIndex(ButtonSiblingIdx);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void SetWarpExit()[m
[32m+[m[32m    {[m
[32m+[m[32m        if (TutorialTile[TutorialTileIdx].GetComponent<TutorialTile>().objName.Equals("Warp_Exit"))[m
[32m+[m[32m        {[m
[32m+[m[32m            InitDisplay(false);[m
[32m+[m[32m            SetButton(false);[m
[32m+[m
[32m+[m[32m            TutorialTile[0].GetComponent<TutorialTile>().lastColList.Clear();[m
[32m+[m
[32m+[m[32m            Cnt = 1;[m
[32m+[m[32m            DisplayIdx = 1;[m
[32m+[m[32m            ButtonIdx = 1;[m
[32m+[m[32m            TutorialTileIdx = 0;[m
[32m+[m
[32m+[m[32m            StopAllCoroutines();[m
[32m+[m[32m            print("All Stop + Start");[m
[32m+[m[32m            StartCoroutine(TakeTask());[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void StartDisplay() //워프 엑싯의 경우에는 ClickSlot이 아니라 MouseDown으로 처리[m
[32m+[m[32m    {[m
[32m+[m[32m        TaskManager.taskManager.OnOffSign(TaskManager.Type.Slide, false);[m
[32m+[m[41m        [m
[32m+[m[32m        DisplayObject[DisplayIdx].SetActive(false);[m
[32m+[m[32m        isClick = true;[m
[32m+[m[32m        SetSlotStatus(false);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void OnDisplay(bool isDisplay) //워프 엑싯의 경우에는 CreateObject가 아니라 MouseUp으로 처리[m
[32m+[m[32m    {[m
[32m+[m[32m        this.isDisplay = isDisplay;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    bool IsDisplay()[m
[32m+[m[32m    {[m
[32m+[m[32m        return isDisplay;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    bool IsDisplayWarpExit()[m
[32m+[m[32m    {[m
[32m+[m[32m        if (Input.GetMouseButtonUp(0))[m
[32m+[m[32m        {[m
[32m+[m[32m            return TutorialTile[TutorialTileIdx].GetComponent<TutorialTile>().isSuccess();[m
[32m+[m[32m        }[m
[32m+[m[32m        else[m
[32m+[m[32m            return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    void InitDisplay(bool isStart)[m
[32m+[m[32m    {[m
[32m+[m[32m        TaskManager.taskManager.isMove = isStart;[m
[32m+[m[32m        RoomManager.possibleDrag = isStart;[m
[32m+[m
[32m+[m[32m        TutorialTile[TutorialTileIdx].SetActive(isStart);[m
[32m+[m[32m        TaskManager.taskManager.createPopUp.isTutorial = isStart;[m
[32m+[m
[32m+[m[32m        if (isStart)[m
[32m+[m[32m        {[m
[32m+[m[32m            TaskManager.taskManager.createPopUp.tutorialTile = TutorialTile[TutorialTileIdx].GetComponent<TutorialTile>();[m
[32m+[m[32m            TaskManager.taskManager.createPopUp.taskObject = this;[m
[32m+[m
[32m+[m[32m            TaskManager.taskManager.createObject.tutorialTile = TutorialTile[TutorialTileIdx].GetComponent<TutorialTile>();[m
[32m+[m[32m            TaskManager.taskManager.createObject.taskObject = this;[m
[32m+[m
[32m+[m[32m            TaskManager.taskManager.createPopUp.transform.SetParent(TaskManager.taskManager.Mask);[m
[32m+[m[32m        }[m
[32m+[m[32m        else[m
[32m+[m[32m        {[m
[32m+[m[32m            TaskManager.taskManager.createPopUp.tutorialTile = null;[m
[32m+[m[32m            TaskManager.taskManager.createPopUp.taskObject = null;[m
[32m+[m
[32m+[m[32m            TaskManager.taskManager.createObject.tutorialTile = null;[m
[32m+[m[32m            TaskManager.taskManager.createObject.taskObject = null;[m
[32m+[m
[32m+[m[32m            TaskManager.taskManager.createPopUp.transform.SetParent(TaskManager.taskManager.CreatePopUpParent);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    bool IsMouseDown()[m
[32m+[m[32m    {[m
[32m+[m[32m        return Input.GetMouseButtonDown(0) || Input.GetMouseButton(0);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    IEnumerator MouseDown()[m
[32m+[m[32m    {[m
[32m+[m[32m        yield return new WaitUntil(IsMouseDown);[m
[32m+[m[32m        StartDisplay();[m
[32m+[m[32m        yield break;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m    IEnumerator Display()[m
[32m+[m[32m    {[m
[32m+[m[32m        isDisplay = false;[m
[32m+[m[32m        isClick = false;[m
[32m+[m[32m        InitDisplay(true);[m
[32m+[m
[32m+[m[32m        bool isWarpExit = TutorialTile[TutorialTileIdx].GetComponent<TutorialTile>().objName.Equals("Warp_Exit");[m
[32m+[m[32m        if (isWarpExit)[m
[32m+[m[32m            RoomManager.possibleDrag = false;[m
[32m+[m
[32m+[m[32m        TaskManager.taskManager.OnOffSign(TaskManager.Type.Slide, true);[m
[32m+[m[32m        SetSlotStatus(true);[m
[32m+[m
[32m+[m[32m        if (!isWarpExit)[m
[32m+[m[32m        {[m
[32m+[m[32m            while (!isClick)[m
[32m+[m[32m            {[m
[32m+[m[32m                DisplayObject[DisplayIdx].SetActive(!DisplayObject[DisplayIdx].activeInHierarchy);[m
[32m+[m[32m                yield return new WaitForSeconds(TaskManager.taskManager.loopArrowSpeed);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        else[m
[32m+[m[32m        {[m
[32m+[m[32m            StartCoroutine(MouseDown());[m
[32m+[m[32m            while (!isClick)[m
[32m+[m[32m            {[m
[32m+[m[32m                DisplayObject[DisplayIdx].SetActive(!DisplayObject[DisplayIdx].activeInHierarchy);[m
[32m+[m[32m                yield return new WaitForSeconds(TaskManager.taskManager.loopArrowSpeed);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!isWarpExit)[m
[32m+[m[32m            yield return new WaitUntil(IsDisplay);[m
[32m+[m[32m        else[m
[32m+[m[32m            yield return new WaitUntil(IsDisplayWarpExit);[m
[32m+[m
[32m+[m[32m        DisplayIdx++;[m
[32m+[m
[32m+[m[32m        InitDisplay(false);[m
[32m+[m[32m        SetSlotStatus(false);[m
[32m+[m[32m        TutorialTileIdx++;[m
[32m+[m[32m        ButtonIdx++;[m
[32m+[m
[32m+[m[32m        isDone = true;[m
[32m+[m[32m        yield break;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void OnResult()[m
[32m+[m[32m    {[m
[32m+[m[32m        isResult = true;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    bool IsResult()[m
[32m+[m[32m    {[m
[32m+[m[32m        return isResult;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    IEnumerator Fight()[m
[32m+[m[32m    {[m
[32m+[m[32m        isResult = false;[m
[32m+[m[32m        resultPopUp.taskObject = this;[m
[32m+[m
[32m+[m[32m        //yield return new WaitForSeconds(2f);[m
[32m+[m[32m        //dayAndNight.changeState();[m
[32m+[m
[32m+[m[32m        yield return new WaitUntil(IsResult);[m
[32m+[m[32m        resultPopUp.taskObject = null;[m
[32m+[m[32m        isDone = true;[m
[32m+[m[32m        yield break;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    bool IsLongClick()[m
[32m+[m[32m    {[m
[32m+[m[32m        return isLongClick;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void OnLongSelect()[m
[32m+[m[32m    {[m
[32m+[m[32m        isLongClick = true;[m
[32m+[m[32m        TaskManager.taskManager.OnOffSign(TaskManager.Type.LongSelect, false);[m
[32m+[m[32m        DisplayObject[DisplayIdx].SetActive(false);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    IEnumerator LongSelect()[m
[32m+[m[32m    {[m
[32m+[m[32m        isLongClick = false;[m
[32m+[m[32m        TaskManager.taskManager.OnOffSign(TaskManager.Type.LongSelect, true);[m
[32m+[m
[32m+[m[32m        ClickObject.taskObject = this;[m
[32m+[m[32m        ClickObject.isPossibleClick = true;[m
[32m+[m[32m        TaskManager.taskManager.isPossibleClick = true;[m
[32m+[m
[32m+[m
[32m+[m[32m        while (!isLongClick)[m
[32m+[m[32m        {[m
[32m+[m[32m            DisplayObject[DisplayIdx].SetActive(!DisplayObject[DisplayIdx].activeInHierarchy);[m
[32m+[m[32m            yield return new WaitForSeconds(TaskManager.taskManager.loopArrowSpeed);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        DisplayIdx++;[m
[32m+[m
[32m+[m[32m        ClickObject.taskObject = null;[m
[32m+[m[32m        ClickObject.isPossibleClick = false;[m
[32m+[m[32m        TaskManager.taskManager.isPossibleClick = false;[m
[32m+[m
[32m+[m[32m        isDone = true;[m
[32m+[m[32m        yield break;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    Vector3 GetRay()[m
[32m+[m[32m    {[m
[32m+[m[32m        Vector3 mousePos = Input.mousePosition;[m
[32m+[m[32m        mousePos.z = 0;[m
[32m+[m[32m        Vector3 pos = Camera.main.ScreenToWorldPoint(mousePos);[m
[32m+[m
[32m+[m[32m        return pos;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    bool isRightDirection()[m
[32m+[m[32m    {[m
[32m+[m[32m        float preDistance = (new Vector3(14.9f, 10f, 7.4f) - roomManager.prePos).magnitude;[m
[32m+[m[32m        float postDistance = (new Vector3(14.9f, 10f, 7.4f) - roomManager.conPos).magnitude;[m
[32m+[m[32m        if (preDistance <= postDistance)[m
[32m+[m[32m            return true;[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    bool isMoveMap()[m
[32m+[m[32m    {[m
[32m+[m[32m        if ((Camera.main.transform.position - new Vector3(14.9f, 10f, 7.4f)).magnitude < 2f)[m
[32m+[m[32m            return true;[m
[32m+[m[32m        else[m
[32m+[m[32m        {[m
[32m+[m[32m            if (Input.GetMouseButton(0))[m
[32m+[m[32m            {[m
[32m+[m[32m                roomManager.conPos = GetRay();[m
[32m+[m[32m                if (isRightDirection())[m
[32m+[m[32m                    Camera.main.transform.position += (roomManager.prePos - roomManager.conPos);[m
[32m+[m[32m                roomManager.prePos = roomManager.conPos;[m
[32m+[m[32m            }[m
[32m+[m[32m            return false;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    IEnumerator MoveMap()[m
[32m+[m[32m    {[m
[32m+[m[32m        TaskManager.taskManager.OnOffSign(TaskManager.Type.Slide, true);[m
[32m+[m
[32m+[m[32m        for (int i = 0; i < 3; i++)[m
[32m+[m[32m        {[m
[32m+[m[32m            DisplayObject[DisplayIdx].SetActive(!DisplayObject[DisplayIdx].activeInHierarchy);[m
[32m+[m[32m            yield return new WaitForSeconds(TaskManager.taskManager.arrowSpeed);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        roomManager.prePos = GetRay();[m
[32m+[m[32m        roomManager.conPos = GetRay();[m
[32m+[m
[32m+[m[32m        yield return new WaitUntil(isMoveMap);[m
[32m+[m
[32m+[m[32m        TaskManager.taskManager.OnOffSign(TaskManager.Type.Slide, false);[m
[32m+[m[32m        DisplayObject[DisplayIdx].SetActive(false);[m
[32m+[m[32m        DisplayIdx++;[m
[32m+[m
[32m+[m[32m        isDone = true;[m
[32m+[m[32m        yield break;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    void SetButton(bool isPossible)[m
[32m+[m[32m    {[m
[32m+[m[32m        SlotManager slotManager = ButtonList[ButtonIdx].GetComponent<SlotManager>();[m
[32m+[m
[32m+[m[32m        if (isPossible)[m
[32m+[m[32m        {[m
[32m+[m[32m            ButtonParent = ButtonList[ButtonIdx].parent;[m
[32m+[m[32m            ButtonSiblingIdx = ButtonList[ButtonIdx].GetSiblingIndex();[m
[32m+[m[32m            if (slotManager != null)[m
[32m+[m[32m            {[m
[32m+[m[32m                slotManager.taskObject = this;[m
[32m+[m[32m                slotManager.isClick = true;[m
[32m+[m[32m            }[m
[32m+[m[32m            else[m
[32m+[m[32m                ButtonList[ButtonIdx].GetComponent<Button>().onClick.AddListener(OnButtonClick);[m
[32m+[m
[32m+[m[32m            ButtonList[ButtonIdx].SetParent(TaskManager.taskManager.Mask);[m
[32m+[m[32m        }[m
[32m+[m[32m        else[m
[32m+[m[32m        {[m
[32m+[m[32m            if (slotManager != null)[m
[32m+[m[32m            {[m
[32m+[m[32m                slotManager.taskObject = null;[m
[32m+[m[32m                slotManager.isClick = false;[m
[32m+[m[32m            }[m
[32m+[m[32m            else[m
[32m+[m[32m                ButtonList[ButtonIdx].GetComponent<Button>().onClick.RemoveListener(OnButtonClick);[m
[32m+[m
[32m+[m[32m            ButtonList[ButtonIdx].SetParent(ButtonParent);[m
[32m+[m[32m            ButtonList[ButtonIdx].SetSiblingIndex(ButtonSiblingIdx);[m
[32m+[m
[32m+[m[32m            ButtonIdx++;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    bool IsClick()[m
[32m+[m[32m    {[m
[32m+[m[32m        return isClick;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void OnButtonClick()[m
[32m+[m[32m    {[m
[32m+[m[32m        isClick = true;[m
[32m+[m[32m        TaskManager.taskManager.OnOffSign(TaskManager.Type.Select, false);[m
[32m+[m[32m        DisplayObject[DisplayIdx].SetActive(false);[m
[32m+[m
[32m+[m[32m        SetButton(false);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    IEnumerator Select()[m
[32m+[m[32m    {[m
[32m+[m[32m        isClick = false;[m
[32m+[m
[32m+[m[32m        if (ButtonList.Count == 0)[m
[32m+[m[32m            ButtonList.Insert(0, TaskManager.taskManager.OriginalMap.transform.GetChild(2));[m
[32m+[m[32m        else if (ButtonList[ButtonIdx].name.Equals("MiniMap"))[m
[32m+[m[32m            yield return new WaitForSeconds(2.3f);[m
[32m+[m[41m        [m
[32m+[m
[32m+[m[32m        SetButton(true);[m
[32m+[m[32m        TaskManager.taskManager.OnOffSign(TaskManager.Type.Select, true);[m
[32m+[m
[32m+[m[32m        while(!isClick)[m
[32m+[m[32m        {[m
[32m+[m[32m            DisplayObject[DisplayIdx].SetActive(!DisplayObject[DisplayIdx].activeInHierarchy);[m
[32m+[m[32m            yield return new WaitForSeconds(TaskManager.taskManager.loopArrowSpeed);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        DisplayIdx++;[m
[32m+[m
[32m+[m[32m        isDone = true;[m
[32m+[m[32m        yield break;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
